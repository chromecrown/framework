{
  "name": "flower/framework",
  "type": "library",
  "description": "flower php api framework",
  "keywords": ["php", "framework", "flower", "api", "swoole"],
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "ext-redis": "*",
    "ext-msgpack": "*",
    "ext-swoole": "~1.9",
    "weipaitang/container": "~0.1|dev-master",
    "weipaitang/server": "~0.1|dev-master",
    "weipaitang/console": "~0.1|dev-master",
    "weipaitang/coroutine": "~0.1|dev-master",
    "weipaitang/http": "~0.1|dev-master",
    "weipaitang/middleware": "~0.1|dev-master",
    "weipaitang/packet": "~0.1|dev-master",
    "weipaitang/config": "~0.1|dev-master",
    "weipaitang/pool": "~0.1|dev-master",
    "weipaitang/client": "dev-master",
    "weipaitang/route": "~0.1|dev-master",
    "weipaitang/log": "~0.1|dev-master",
    "weipaitang/lock": "~0.1|dev-master",
    "weipaitang/database": "~0.1|dev-master"
  },
  "autoload": {
    "psr-4": {
      "Weipaitang\\Framework\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "secure-http": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
